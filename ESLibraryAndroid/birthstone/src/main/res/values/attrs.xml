    <declare-styleable name="ESTextView">
        <attr name="dataType"></attr>
        <attr name="isRequired"></attr>
        <attr name="empty2Null"></attr>
        <attr name="collectSign"></attr>
    </declare-styleable>

    <declare-styleable name="ESRelativeLayout">
        <!--状态ViewID-->
        <attr name="stateHiddenId"></attr>
        <!--希望显示状态的值-->
        <attr name="wantedStateValue"></attr>
        <!--权限标记-->
        <attr name="funcSign"></attr>
    </declare-styleable>

    <declare-styleable name="ESLinearLayout">
        <!--状态ViewID-->
        <attr name="stateHiddenId"></attr>
        <!--希望显示状态的值-->
        <attr name="wantedStateValue"></attr>
        <!--权限标记-->
        <attr name="funcSign"></attr>
    </declare-styleable>

    <declare-styleable name="ESMaterialTabHost">
        <attr name="tabHost_height" format="dimension"></attr>
        <attr name="tabHost_BackgroundColor" format="color"></attr>
        <attr name="tabHost_titleDefaultColor" format="color"></attr>
        <attr name="tabHost_titleActiveColor" format="color"></attr>
        <attr name="tabHost_titleSize" format="float"></attr>
        <attr name="tabHost_indexerColor" format="color"></attr>
        <attr name="tabHost_indexerHeight" format="dimension"></attr>
        <attr name="tabHost_boldText" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="ESPhotoAddView">
        <attr name="imageMaxCount" format="integer"></attr>
        <attr name="imageHeight" format="dimension"></attr>
        <attr name="imageWidth" format="dimension"></attr>
        <attr name="bitmapCachePath" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="ESNumberPickerView">
        <attr name="numberBackground" format="reference"></attr>
        <attr name="button_add_img" format="reference"></attr>
        <attr name="button_minus_img" format="reference"></attr>
        <attr name="numberTextColor" format="color"></attr>
        <attr name="numberTextSize" format="float"></attr>
        <attr name="editable" format="boolean"></attr>
        <attr name="maxValue" format="integer"></attr>
        <attr name="minValue" format="integer"></attr>
        <attr name="collectSign"></attr>
    </declare-styleable>

    <declare-styleable name="ESImageView">
        <attr name="srcid" format="reference"></attr>
        <attr name="collectSign"></attr>
    </declare-styleable>

    <declare-styleable name="ESRatingBar">
        <attr name="dataType"></attr>
        <attr name="isRequired"></attr>
        <attr name="empty2Null"></attr>
        <attr name="collectSign"></attr>
        <!--必填项错误提示-->
        <attr name="isRequiredTooltip"></attr>
        <!--正则表达式-->
        <attr name="regularExpression"></attr>
        <!--必填项提示-->
        <attr name="hint" format="string"></attr>
        <!--正则表达式校验错误提示-->
        <attr name="regularTooltip"></attr>
    </declare-styleable>

    <declare-styleable name="ESRatingBarBase">
        <!--尺寸值-->
        <attr name="starImageSize" format="dimension" />
        <!--星星间距-->
        <attr name="starPadding" format="dimension" />
        <!--星星总数-->
        <attr name="starCount" format="integer" />
        <!--空白的星星资源文件值-->
        <attr name="starEmpty" format="reference" />
        <!--满星资源文件值-->
        <attr name="starFill" format="reference" />
        <!--半星资源文件值-->
        <attr name="starHalf" format="reference" />
        <!--是否可点击boolean值-->
        <attr name="clickable" format="boolean" />
        <!--当前进度float值-->
        <attr name="starStep" format="float" />
        <!--每次进度方式的值，整星还是半星-->
        <attr name="stepSize">
            <enum name="Half" value="0" />
            <enum name="Full" value="1" />
        </attr>
    </declare-styleable>
</resources>
